@model IEnumerable<BikeShowroom.Models.Bikes>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.BikeNames)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BikeModel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BikeVerison)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.BikeNames)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BikeModel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BikeVerison)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.BikeNums">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.BikeNums">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.BikeNums">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
